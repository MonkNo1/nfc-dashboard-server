# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Configuration
MONGO_URI=mongodb+srv://admin:projectpassword@your_cluster.mongodb.net/nfc-dashboard?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_EXPIRES_IN=7d

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Session Configuration
SESSION_SECRET=your_session_secret_key_change_this_in_production
SESSION_COOKIE_MAX_AGE=86400000 # 24 hours in milliseconds

# Frontend URL (for CORS and redirects)
FRONTEND_BASE_URL=http://localhost:3000

# Security
CORS_ORIGIN=http://localhost:3000,https://nfc-dashboard-five.vercel.app 
